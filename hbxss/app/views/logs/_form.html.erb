<%=

if (params[:key])
	@log = Log.new
	@log.key = params[:key].to_s
	@log.url = params[:url].to_s
	@log.referer = params[:referer].to_s
	@log.ip = "Not support"#params[:key].to_s
	@log.time = params[:time].to_s
	@log.save
	
 end

%>

  <div class="field">
    <%= form.label :key %>
    <%= form.text_field :key, id: :log_key %>
